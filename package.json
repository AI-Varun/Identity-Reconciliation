{
  "name": "identity-reconciliation",
  "version": "0.1.0",
  "description": "Identify and keep track of a customer's identity across multiple purchases",
  "author": "Raj Varun",
  "license": "MIT",
  "keywords": [
    "typescript",
    "node",
    "express",
    "sequelize",
    "mysql",
    "template",
    "typescript node sequelize boilerplate",
    "typescript node",
    "typescript sequelize boilerplate"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "dev": "tsnd --respawn src/server.ts"
  },
  "dependencies": {
    "@types/node": "^20.8.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "sequelize": "^6.25.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/concurrently": "5.2.1",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/morgan": "^1.9.3",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "6.0.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
